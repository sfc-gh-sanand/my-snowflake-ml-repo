import snowflake.snowpark as snowpark
import sys

def main(session: snowpark.Session):
    # 1. Debug: See exactly what Snowflake sent
    print("--- Debugging sys.argv ---")
    for i, arg in enumerate(sys.argv):
        print(f"Index {i}: {arg}")
    
    # 2. Process: Split the string by spaces to get separate variables
    # We join all arguments and split to handle any Snowflake format
    all_args = " ".join(sys.argv[1:]).split()
    
    if len(all_args) >= 3:
        target_db = all_args[0]
        target_schema = all_args[1]
        target_wh = all_args[2]
        
        # 3. Apply: Move the session to the new environment
        session.use_database(target_db)
        session.use_schema(target_schema)
        session.use_warehouse(target_wh)
        
        return f"Successfully moved to {target_db}.{target_schema}"
    else:
        return f"Error: Expected 3 arguments, but got {len(all_args)}"
